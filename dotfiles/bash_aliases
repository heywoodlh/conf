alias anonymous="proxychains -q /bin/bash"
alias buku-fix="rm -rf ~/.local/share/buku && git clone git@git.kube:heywoodlh/buku ~/.local/share/buku"
alias edit="$EDITOR $@"
alias k="kubectl"
alias kg='kubectl get'
alias kl='kubectl logs '
alias kx='kubectl exec -i -t'
alias notify="ntfy send $@"
alias proxychains='proxychains -q $@'
alias scratch='vim /tmp/scratch.txt'
alias tf="terraform"
alias tfa="terraform apply"
alias tfp="terraform plan"
alias tfw="terraform workspace"
alias tls="tmux ls"
alias todo="todo.sh ls | sort -u"
alias todone="todo.sh lsa | grep '00 x' | cut -f3- -d' '"
alias weather='curl http://wttr.in/Manti'
